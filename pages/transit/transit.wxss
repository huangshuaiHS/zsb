map{
  width: 100%;
  height: calc( 100vh - 90rpx);
}
 
.catalog{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  background: #000;
}
.catalog .item{
  float: left;
  width: 16.6666%;
  color: #fff;
  text-align: center;
  font-size: 30rpx;
  line-height: 90rpx;
}
.catalog .item.active{
  background: #de3435;
}